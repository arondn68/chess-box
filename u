<script>
  $(document).ready(function(){
    console.log("clicked");
    $(".piece-promotion").click(function(){
      var olgaSource = $("#pawn-promotion-selection").attr("data-olga-source");
      var olgaTarget = $("#pawn-promotion-selection").attr("data-olga-target");
      var olgaPromotionValue = $(this).attr("id");
      OLGA_MAIN.formerOnDropPromotion(olgaSource, olgaTarget, olgaPromotionValue);
      $("#prevB").click();
      $("#nextB").click();
      $("#pawn-promotion-selection").hide();
    })
  })

</script>
